{
  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "route": "profile/{userId:alpha}",
      "methods": [
        "post"
      ]
    },
    {
      "name": "userInfo",
      "type": "blob",
      "path": "profiles/{UserId}.json",
      "connection": "PROFILES_STORAGE_CONNECTION",
      "direction": "out"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/postProfile/index.js"
}
